    "@graphql-tools/load-files": "^6.6.1",
    "@graphql-tools/merge": "^8.3.12",
    "@keyv/redis": "^2.5.3",
    "@prisma/client": "^4.5.0",
    "bcrypt": "^5.0.1",
    "connect-redis": "^6.1.3",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv-flow": "^3.2.0",
    "envalid": "^7.3.1",
    "eslint-config-airbnb": "^19.0.4",
    "express": "~4.16.1",
    "express-rate-limit": "^6.4.0",
    "express-session": "^1.17.3",
    "graphql": "^16.6.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-middleware": "^6.1.31",
    "graphql-rate-limit": "^3.3.0",
    "graphql-request": "^4.3.0",
    "graphql-scalars": "^1.17.0",
    "graphql-shield": "^7.5.0",
    "graphql-tools": "^8.2.12",
    "graphql-upload": "^16.0.2",
    "graphql-voyager": "1.0.0-rc.31",
    "http-errors": "~1.6.3",
    "ioredis": "^5.2.3",
    "jsonwebtoken": "^8.5.1",
    "keyv": "^4.5.0",
    "minio": "^7.0.32",
    "morgan": "~1.9.1",
    "prisma": "^4.5.0",
    "redis": "^4.2.0",
    "uuid": "^8.3.2",
    "voyager": "^0.4.13"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.14.1",

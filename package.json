{
  "name": "back",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "dev": "npm-run-all dev:*",
    "release": "npm-run-all release:*",
    "release:db": "npx prisma migrate deploy",
    "codegen": "graphql-codegen --config codegen.ts",
    "watch": "concurrently --names \"Codegen,TS,Nodemon,GraphQLi\" -c \"bgYellow,bgBlue,bgMagneta,bgBlack\" \"graphql-codegen --watch './src/graphql/**/*.graphql'\" \"nodemon\" \"ts-node \"src\\scripts\\spawn_graphqli.ts\"\" --kill-others"
  },
  "dependencies": {
    "@apollo/server": "^4.2.1",
    "@apollo/utils.keyvadapter": "^1.1.2",
    "@graphql-tools/graphql-file-loader": "^7.5.11",
